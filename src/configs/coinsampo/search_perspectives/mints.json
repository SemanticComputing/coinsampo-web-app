{
    "id": "mints",
    "endpoint": {
        "url": "https://ldf.fi/coinsampo/sparql",
        "useAuth": true,
        "prefixesFile": "SparqlQueriesPrefixes.js"
    },
    "sparqlQueriesFile": "SparqlQueriesMints.js",
    "baseURI": "http://ldf.fi/coinsampo",
    "URITemplate": "<BASE_URI>/mints/<LOCAL_ID>",
    "facetClass": "coin-schema:Mint",
    "frontPageImage": "main_page/taonta.png",
    "searchMode": "faceted-search",
    "enableDynamicLanguageChange": true,
    "defaultActiveFacets": [
        "prefLabel"
    ],
    "defaultTab": "table",
    "defaultInstancePageTab": "table",
    "resultClasses": {
        "mints": {
            "paginatedResultsConfig": {
                "tabID": 0,
                "component": "ResultTable",
                "tabPath": "table",
                "tabIcon": "CalendarViewDay",
                "propertiesQueryBlock": "mintsPropertiesFacetResults",
                "pagesize": 10,
                "sortBy": null,
                "sortDirection": "asc"
            },
            "instanceConfig": {
                "propertiesQueryBlock": "mintPropertiesInstancePage",
                "instancePageResultClasses": {
                    "instancePageTable": {
                        "tabID": 0,
                        "component": "InstancePageTable",
                        "tabPath": "table",
                        "tabIcon": "CalendarViewDay"
                    }
                }
            }           
        },
        "mintsPlaces": {
            "tabID": 1,
            "tabPath": "map",
            "tabIcon": "AddLocation",
            "component": "LeafletMap",
            "showInstanceCountInClusters": false,
            "showExternalLayers": true,
            "customMapControl": true,
            "sparqlQuery": "mintsPlacesQuery",
            "filterTarget": "id",
            "facetClass": "mints",
            "mapMode": "marker",
            "resultMapper": "mapPlaces",
            "instanceConfig": {
                "propertiesQueryBlock": "mintPropertiesInstancePage",
                "createPopUpContent": "createPopUpContentFindSampo"
            }
        },
        "findsKnowledgeGraphMetadata": {
            "sparqlQuery": "knowledgeGraphMetadataQuery",
            "resultMapper": "makeObjectList"
        }
    },
    "maps": {
        "mintsPlaces": {
            "center": [
                65.184809,
                27.314050
            ],
            "zoom": 5
        },
        "findsPlacesHeatmap": {
            "center": [
                65.184809,
                27.314050
            ],
            "zoom": 4
        },
        "findInstancePageMap": {
            "center": [
                65.184809,
                27.314050
            ],
            "zoom": 5
        },
        "findInstancePageNearbyFinds": {
            "center": [
                65.184809,
                27.314050
            ],
            "zoom": 5
        }
    },
    "properties": [
        {
            "id": "prefLabel",
            "valueType": "object",
            "makeLink": true,
            "externalLink": false,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 140
        },
        {
            "id": "material",
            "valueType": "object",
            "makeLink": true,
            "externalLink": false,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 180,
            "onlyOnInstancePage": true
        },
        {
            "id": "denomination",
            "valueType": "object",
            "makeLink": true,
            "externalLink": false,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 130,
            "onlyOnInstancePage": true
        },
        {
            "id": "period",
            "valueType": "object",
            "makeLink": true,
            "externalLink": false,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 130
        },
        {
            "id": "authority",
            "valueType": "object",
            "makeLink": true,
            "externalLink": false,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 130
        },
        {
            "id": "municipality",
            "valueType": "object",
            "makeLink": true,
            "externalLink": false,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 130,
            "onlyOnInstancePage": true
        },
        {
            "id": "find",
            "valueType": "object",
            "makeLink": true,
            "externalLink": false,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 130,
            "onlyOnInstancePage": true
        }
    ],
    "facets": {
        "denomination": {
            "containerClass": "ten",
            "facetType": "list",
            "filterType": "uriFilter",
            "predicate": "^coin-schema:mint/coin-schema:denomination",
            "searchField": true,
            "sortButton": true,
            "sortBy": "instanceCount",
            "sortByPredicate": "^coin-schema:mint/coin-schema:denomination/skos:prefLabel",
            "facetLabelFilter": "FILTER(LANG(?prefLabel_) = '<LANG>')",
            "sortDirection": "desc",
            "pieChartButton": true
        },
        "municipality": {
            "containerClass": "ten",
            "facetType": "list",
            "filterType": "uriFilter",
            "predicate": "^coin-schema:mint/coin-schema:municipality/coin-schema:yso",
            "searchField": true,
            "sortButton": true,
            "sortBy": "instanceCount",
            "sortByPredicate": "^coin-schema:mint/coin-schema:municipality/coin-schema:yso/skos:prefLabel",
            "facetLabelFilter": "FILTER(LANG(?prefLabel_) = '<LANG>')",
            "sortDirection": "desc",
            "pieChartButton": true
        },
        "period": {
            "containerClass": "ten",
            "facetType": "list",
            "filterType": "uriFilter",
            "predicate": "^coin-schema:mint/coin-schema:period",
            "searchField": true,
            "sortButton": true,
            "sortBy": "instanceCount",
            "sortByPredicate": "^coin-schema:mint/coin-schema:period/skos:prefLabel",
            "facetLabelFilter": "FILTER(LANG(?prefLabel_) = '<LANG>')",
            "sortDirection": "desc",
            "pieChartButton": true
        },
        "authority": {
            "containerClass": "ten",
            "facetType": "list",
            "filterType": "uriFilter",
            "predicate": "^coin-schema:mint/coin-schema:authority",
            "searchField": true,
            "sortButton": true,
            "sortBy": "instanceCount",
            "sortByPredicate": "^coin-schema:mint/coin-schema:authority/skos:prefLabel",
            "facetLabelFilter": "FILTER(LANG(?prefLabel_) = '<LANG>')",
            "sortDirection": "desc",
            "pieChartButton": true
        },
        "material": {
            "containerClass": "ten",
            "facetType": "list",
            "filterType": "uriFilter",
            "predicate": "^coin-schema:mint/coin-schema:material",
            "searchField": true,
            "sortButton": true,
            "sortBy": "instanceCount",
            "sortByPredicate": "^coin-schema:mint/coin-schema:material/skos:prefLabel",
            "facetLabelFilter": "FILTER(LANG(?prefLabel_) = '<LANG>')",
            "sortDirection": "desc",
            "pieChartButton": true
        }
    }
}