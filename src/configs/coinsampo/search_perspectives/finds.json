                    "resultMapper": "mapAoristicChart",
                    "sliceVisibilityThreshold": false,
                    "dropdownForChartTypes": false,
                    "chartTypes": [
                        {
                            "id": "bar",
                            "createChartData": "createApexBarChartAoristictData"
                        }
                    ]
                }
            }
