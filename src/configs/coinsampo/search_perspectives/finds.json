        "coinsMigrations": {
            "tabID": 7,
            "component": "Deck",
            "tabPath": "migrations",
            "tabIcon": "Redo",
            "sparqlQuery": "migrationsQuery",
            "facetClass": "finds",
            "filterTarget": "coin",
            "resultMapper": "makeObjectList",
            "layerType": "arcLayer",
            "arcWidthVariable": "instanceCountScaled",
            "instanceVariable": "coin",
            "showTooltips": false,
            "postprocess": {
                "func": "linearScale",
                "config": {
                    "variable": "instanceCount",
                    "minAllowed": 2,
                    "maxAllowed": 20
                }
            }
        },
        "coinsMigrationsDialog": {
            "sparqlQuery": "migrationsDialogQuery",
            "filterTarget": "id",
