{
    "id": "finds",
    "endpoint": {
        "url": "https://ldf.fi/coinsampo/sparql",
        "useAuth": true,
        "prefixesFile": "SparqlQueriesPrefixes.js"
    },
    "sparqlQueriesFile": "SparqlQueriesFinds.js",
    "baseURI": "http://ldf.fi/coinsampo",
    "URITemplate": "<BASE_URI>/coins/<LOCAL_ID>",
    "facetClass": "coin-schema:Coin",
    "frontPageImage": "main_page/kuusinainen.png",
    "enableDynamicLanguageChange": true,
    "searchMode": "faceted-search",
    "defaultActiveFacets": [
        "prefLabel"
    ],
    "defaultTab": "table",
    "defaultInstancePageTab": "table",
    "resultClasses": {
        "finds": {
            "paginatedResultsConfig": {
                "tabID": 0,
                "component": "ResultTable",
                "tabPath": "table",
                "tabIcon": "CalendarViewDay",
                "propertiesQueryBlock": "findPropertiesFacetResults",
                "pagesize": 10,
                "sortBy": null,
                "sortDirection": "asc"
            },
            "instanceConfig": {
                "propertiesQueryBlock": "findPropertiesFacetResults",
                "instancePageResultClasses": {
                    "instancePageTable": {
                        "tabID": 0,
                        "component": "InstancePageTable",
                        "tabPath": "table",
                        "tabIcon": "CalendarViewDay"
                    }
                }
            }           
        },
        "findsPlaces": {
            "tabID": 2,
            "tabPath": "map",
            "tabIcon": "AddLocation",
            "component": "LeafletMap",
            "showInstanceCountInClusters": false,
            "showExternalLayers": true,
            "customMapControl": true,
            "sparqlQuery": "findsPlacesQuery",
            "filterTarget": "id",
            "facetClass": "finds",
            "resultMapper": "mapPlaces",
            "instanceConfig": {
                "propertiesQueryBlock": "findPropertiesFacetResults",
                "createPopUpContent": "createPopUpContentFindSampo"
            }
        },
        "findsPlacesHeatmap": {
            "tabID": 3,
            "tabPath": "heatmap",
            "tabIcon": "BubbleChart",
            "component": "Deck",
            "layerType": "heatmapLayer",
            "sparqlQuery": "findsPlacesQuery",
            "facetClass": "finds",
            "filterTarget": "id",
            "resultMapper": "mapPlaces"
        },
        "findsByProperty": {
            "tabID": 4,
            "component": "ApexCharts",
            "doNotRenderOnMount": true,
            "tabPath": "pie_chart",
            "tabIcon": "PieChart",
            "facetClass": "finds",
            "dropdownForResultClasses": true,
            "defaultResultClass": "findsByPeriod",
            "resultClasses": {
                "findsByPeriod": {
                    "sparqlQuery": "findsByPeriodQuery",
                    "filterTarget": "find",
                    "resultMapper": "mapPieChart",
                    "sliceVisibilityThreshold": 0.01,
                    "dropdownForChartTypes": "true",
                    "chartTypes": [
                        {
                            "id": "pie",
                            "createChartData": "createApexPieChartData"
                        },
                        {
                            "id": "bar",
                            "createChartData": "createApexBarChartData"
                        }
                    ]
                },
                "findsByRuler": {
                    "sparqlQuery": "findsByRulerQuery",
                    "filterTarget": "find",
                    "resultMapper": "mapPieChart",
                    "sliceVisibilityThreshold": 0.01,
                    "dropdownForChartTypes": "true",
                    "chartTypes": [
                        {
                            "id": "pie",
                            "createChartData": "createApexPieChartData"
                        },
                        {
                            "id": "bar",
                            "createChartData": "createApexBarChartData"
                        }
                    ]
                },
                "findsByMint": {
                    "sparqlQuery": "findsByMintQuery",
                    "filterTarget": "find",
                    "resultMapper": "mapPieChart",
                    "sliceVisibilityThreshold": 0.01,
                    "dropdownForChartTypes": "true",
                    "chartTypes": [
                        {
                            "id": "pie",
                            "createChartData": "createApexPieChartData"
                        },
                        {
                            "id": "bar",
                            "createChartData": "createApexBarChartData"
                        }
                    ]
                },
                "findsByContext": {
                    "sparqlQuery": "findsByContextQuery",
                    "filterTarget": "find",
                    "resultMapper": "mapPieChart",
                    "sliceVisibilityThreshold": 0.001,
                    "dropdownForChartTypes": "true",
                    "chartTypes": [
                        {
                            "id": "pie",
                            "createChartData": "createApexPieChartData"
                        },
                        {
                            "id": "bar",
                            "createChartData": "createApexBarChartData"
                        }
                    ]
                }
            }
        },
        "aoristicFinds": {
            "tabID": 5,
            "component": "ApexCharts",
            "doNotRenderOnMount": true,
            "tabPath": "aoristic_chart",
            "tabIcon": "BarChart",
            "facetClass": "finds",
